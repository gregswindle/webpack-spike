{
  "name": "webpack-spike",
  "description": "A sample webpack demo.",
  "version": "1.0.0",
  "author": "Greg Swindle <greg@swindle.net>",
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/gregswindle/webpack-spike/issues"
  },
  "commitplease": {
    "style": "angular",
    "types": [
      "build",
      "chore",
      "ci",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ],
    "scope": "\\S+.*"
  },
  "contributors": [
    "Spencer Hamm <spentacular@gmail.com>"
  ],
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "lodash": "4.17.4",
    "markdown-magic": "0.1.18",
    "markdown-magic-install-command": "1.3.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "clean-webpack-plugin": "0.1.17",
    "commitplease": "2.7.10",
    "css-loader": "0.28.7",
    "fixpack": "2.3.1",
    "html-webpack-plugin": "2.30.1",
    "nsp": "^2.6.3",
    "standard-version": "4.2.0",
    "style-loader": "0.19.0"
  },
  "homepage": "https://github.com/gregswindle/webpack-spike",
  "keywords": [
    "css-loader",
    "loader",
    "style-loader",
    "webpack"
  ],
  "license": "MIT",
  "main": "index.js",
  "maintainers": [],
  "repository": "gregswindle/webpack-spike.git",
  "scripts": {
    "build": "webpack",
    "dev": "webpack -w",
    "docs": "npm run docs:toc",
    "docs:toc": "md-magic --path '**/*.md' --ignore 'node_modules'",
    "lint": "npm run lint:manifest",
    "lint:manifest": "fixpack",
    "prepare": "npm run lint && npm run security",
    "prepublish": "nsp check",
    "security": "npm run security:nsp:scan",
    "security:nsp:scan": "nsp check"
  }
}
